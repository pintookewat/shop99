  <div class="card-body" >
    <%= form_for(resource, as: resource_name, html: {'data-turbo' => "true", id: "user_new"},url: registration_path(resource_name)) do |f| %>
    <!--  <%= render "devise/shared/error_messages", resource: resource %> -->
    <div class="mb-3">
      <%= f.label :full_name , "Full Name*"%>
      <%= f.text_field :full_name, autofocus: true, autocomplete: "full_name", class:"form-control ", placeholder: 'Enter your Name' %>
    </div>
    <div class="field mt-1">
      <%= f.label :email, "Email*"%>
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control", placeholder: 'Enter your Email' %>
    </div>
    <div class="field mt-1">
      <%= f.label :gender, "Gender*", class:"sign_up_field_text" %>
      <br>
      <span class="sign_up_field_text">
        <%= f.radio_button :gender, 'Male' %>
        <%= f.label :gender, 'Male', :value => 'Male' %>
      </span>
      <span class=" ml-3">
        <%= f.radio_button :gender, 'Female' %>
        <%= f.label :gender, 'Female', :value => 'Female' %>
      </span>
      <span class=" ml-3">
        <%= f.radio_button :gender, 'Other' %>
        <%= f.label :gender, 'Other', :value => 'Other' %>
      </span>
    </div>
    <div class="mb-3 ">
      <%= f.label :phone, "Phone*" %>
      <%= f.text_field :phone, autofocus: true, autocomplete: "phone", class:"form-control ", placeholder: 'Enter your Phone' %>
    </div>
    <div class="mb-3">
      <%#= f.label :proffesion, "Proffesion*" %>
      <%#= f.select :proffesion, options_for_select([["Select One", ""], "Student", "Work", "Office", "Home", "Other"]),{}, class:'form-control' %>
    </div>
    <div class="mb-3">
      <%= f.label :password, "Password*" %>
      <%= f.password_field :password, autocomplete: "new-password", class:"form-control", placeholder: 'Enter your Password' %>
    </div>
    <div class="mb-3">
      <%= f.label :password_confirmation, "Password Confirm*" %>
      <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control ", placeholder: 'Enter your Confirm Password' %>
    </div>
    <div class="mb-3">
      <%#= f.label :age, "Age*" %>
      <%#= f.text_field :age, autofocus: true, autocomplete: "phone", class:"form-control", placeholder: 'Enter your Age' %>
    </div>
    <div class="mb-3">
      <%#= f.label :location, "Location*"%>
      <%#= f.text_field :location, autofocus: true, autocomplete: "phone", class:"form-control", placeholder: 'Enter your Location' %>
    </div>
    <div class="mb-3">
      <%= f.submit "Register", class:"btn bg-gradient-dark w-100 my-4 mb-2" %>
    </div>
    <p class="text-sm mt-3 mb-0 text-center">Already have an account? <a href="/users/sign_in" class="text-dark font-weight-bolder">Sign in</a></p>
    <% end %>
  </div>
